{
    "compilerOptions": {
      "lib": ["DOM", "ESNext"],
      "module": "esnext",
      "target": "esnext",
      "moduleResolution": "node",
      "isolatedModules": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts", "src/**/*.svelte"],
    "exclude": ["node_modules"]
  }
  